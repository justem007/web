<?xml version="1.0"?>
<config>
	<modules>
		<RicardoJustem_RicardoModel>
			<version>0.1.0</version>
		</RicardoJustem_RicardoModel>
	</modules>
	<adminhtml>
		<translate>
			<modules>
				<RicardoJustem_RicardoModel>
					<files>
						<default>RicardoJustem_RicardoModel.csv</default>
					</files>
				</RicardoJustem_RicardoModel>
			</modules>
		</translate>
	</adminhtml>
	<global>
		<models>
			<ricardojustem_ricardomodel>
				<class>RicardoJustem_RicardoModel_Model</class>
